<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class  name="net.sf.odinms.server.MaplePortal" table="wz_portal">
        <composite-id>
            <key-property name="type"/>
            <key-property name="id"/>
            <key-property name="mapid"/>
        </composite-id>
        <property name="name" not-null="true"/>
        <property name="target" not-null="true"/>
        <property name="scriptName"/>
        <property name="position" not-null="true"/>
        <property name="targetmap" not-null="true"/>
        <property name="portalState" not-null="true"/>
    </class>
</hibernate-mapping>
